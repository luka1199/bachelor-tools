  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "csv-parser": "^3.0.0",
    "get-stdin": "^6.0.0",
    "mocha": "^8.4.0",
    "nearest-date": "^1.0.1",
